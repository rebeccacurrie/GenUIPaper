{
  "framework": "vite",
  "outputDirectory": "dist",
  "functions": {
    "api/**": {
      "includeFiles": "lib/**"
    }
  },
  "rewrites": [
    { "source": "/api/table", "destination": "/api/table" },
    { "source": "/api/logs", "destination": "/api/logs" },
    { "source": "/api/run-batch", "destination": "/api/run-batch" },
    { "source": "/api/config", "destination": "/api/config" }
  ]
}
